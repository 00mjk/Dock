<Window x:Class="AvaloniaDemo.MainWindow"
        xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:i="clr-namespace:Avalonia.Xaml.Interactivity;assembly=Avalonia.Xaml.Interactivity"
        xmlns:ia="clr-namespace:Avalonia.Xaml.Interactions.Core;assembly=Avalonia.Xaml.Interactions"
        xmlns:id="clr-namespace:Dock.Avalonia;assembly=Dock.Avalonia"
        xmlns:idd="clr-namespace:Dock.Avalonia.Dock;assembly=Dock.Avalonia"
        xmlns:idh="clr-namespace:Dock.Avalonia.Handlers;assembly=Dock.Avalonia"
        xmlns:idc="clr-namespace:Avalonia.Controls;assembly=Dock.Avalonia"
        UseLayoutRounding="True"
        Title="Dock Avalonia Demo" Height="640" Width="640">
    <idd:LayoutControl DataContext="{Binding Layout}"/>
</Window>
